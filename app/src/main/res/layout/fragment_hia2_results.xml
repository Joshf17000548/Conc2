<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="16dp">

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25sp"
            android:text="HIA2 Results"
            android:textStyle="bold"
            android:id="@+id/titleR"
            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_below="@id/titleR"
            android:layout_alignParentEnd="true"
            android:id="@+id/tableHeaders"
            android:layout_toRightOf="@id/IMR">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Res"
                android:textStyle="bold"
                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Base"
                android:textStyle="bold"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Req"
                android:textStyle="bold"
                android:gravity="center"/>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="Symptoms"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_below="@id/tableHeaders"
            android:id="@+id/SYM"
            android:gravity="center"
            android:layout_gravity="start"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:id="@+id/SYMtestResult"
            android:text="Passed"
            android:layout_marginStart="20dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/SYM"
            android:gravity="center"
            android:layout_alignBottom="@id/SYM"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginStart="20dp"
            android:text="# Symptoms"
            android:layout_below="@+id/SYM"
            android:id="@+id/SYMnum"
            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/SYMnumholder"
            android:layout_marginStart="20dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/SYMnum"
            android:layout_alignBottom="@id/SYMnum">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/SYMnumResult"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/SYMnumBaseline"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/SYMnumRequired"
                android:text="TBC"

                android:gravity="center"/>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:textSize="15sp"
            android:text="Total Symptoms"
            android:layout_below="@id/SYMnumholder"
            android:id="@+id/SYMtot"

            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:id="@+id/SYMtotholder"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/SYMtot"
            android:layout_alignBottom="@id/SYMtot">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/SYMtotResult"
                android:text="-"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"

                android:id="@+id/SYMtotBaseline"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/SYMtotRequired"
                android:text="TBC"
                android:gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/SYMtotholder"
            android:background="#000000"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textStyle="bold"
            android:text="Orientation"
            android:layout_marginTop="10dp"
            android:id="@+id/MQ"
            android:gravity="center"
            android:layout_below="@+id/SYMtotholder"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginStart="20dp"
            android:id="@+id/MQholder"
            android:layout_toEndOf="@+id/IMR"
            android:layout_alignTop="@id/MQ"
            android:layout_alignBottom="@id/MQ">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/MQresult"
                android:layout_weight="1"
                android:text="-"

                android:gravity="center"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/MQrequired"
                android:layout_weight="1"
                android:text="5"

                android:gravity="center"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/MQholder"
            android:background="#000000"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:id="@+id/IMR"
            android:textStyle="bold"
            android:text="Immediate Memory"
            android:layout_below="@id/MQholder"
            android:layout_marginTop="10dp"

            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_alignTop="@id/IMR"
            android:layout_marginStart="20dp"
            android:layout_alignBottom="@id/IMR"
            android:id="@+id/IMRholder"
            android:layout_toRightOf="@id/IMR"
            >

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/IMRresult"
                android:layout_weight="1"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/IMRbaseline"
                android:layout_weight="1"
                android:text="-"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:id="@+id/IMRrequired"
                android:layout_weight="1"
                android:text="12"
                android:gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/IMRholder"
            android:background="#000000"/>


        <TextView
            android:id="@+id/DB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="Digits Backwards"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_below="@id/IMRholder"
            android:gravity="center"
            android:layout_gravity="start"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_marginStart="20dp"
            android:layout_alignTop="@id/DB"
            android:layout_alignBottom="@id/DB"
            android:id="@+id/DBholder">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DBresult"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DBbaseline"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="3"
                android:id="@+id/IDBrequired"

                android:gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/DBholder"
            android:background="#000000"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textStyle="bold"
            android:text="Months in Reverse"

            android:id="@+id/MR"
            android:gravity="center"
            android:layout_below="@+id/DBholder"
            android:layout_gravity="start"
            android:layout_marginTop="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginStart="20dp"
            android:id="@+id/MRholder"
            android:layout_toEndOf="@+id/IMR"
            android:layout_alignTop="@id/MR"
            android:layout_alignBottom="@id/MR">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
