                android:id="@+id/TGmlrmsBaseline"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/TGmlrmsRequired"
                android:text="TBC"

                android:gravity="center"/>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:textSize="15sp"
            android:text="AP-RMS"
            android:layout_below="@id/TGmlrmsholder"
            android:id="@+id/TGalrms"

            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:id="@+id/TGalrmsholder"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/TGalrms"
            android:layout_alignBottom="@id/TGalrms">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/TGalrmsResult"
                android:text="-"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"

                android:id="@+id/TGalrmsBaseline"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/TGalrmsRequired"
                android:text="TBC"

                android:gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/TGalrmsholder"
            android:background="#000000"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="Double leg"
            android:layout_below="@id/TGalrmsholder"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:id="@+id/DL"
            android:gravity="center"
            android:layout_gravity="start"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:id="@+id/DLResult"
            android:text="Failed"
            android:layout_marginStart="20dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/DL"
            android:gravity="center"
            android:layout_alignBottom="@id/DL"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginStart="20dp"
            android:text="PTP"
            android:layout_below="@+id/DLResult"
            android:id="@+id/DLptp"
            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/DLptpholder"
            android:layout_marginStart="20dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/DLptp"
            android:layout_alignBottom="@id/DLptp">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DLptpResult"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DLptpBaseline"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/DLptpRequired"
                android:text="TBC"

                android:gravity="center"/>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginStart="20dp"
            android:text="ML-RMS"

            android:layout_below="@+id/DLptpholder"
            android:id="@+id/DLmprms"
            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/DLmprmsholder"
            android:layout_marginStart="20dp"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/DLmprms"
            android:layout_alignBottom="@id/DLmprms">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DLmprmsResult"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"
                android:id="@+id/DLmprmsBaseline"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/DLmprmsRequired"
                android:text="TBC"

                android:gravity="center"/>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:textSize="15sp"
            android:text="AP-RMS"
            android:layout_below="@id/DLmprmsholder"
            android:id="@+id/DLalrms"

            android:gravity="center"
            android:layout_gravity="start"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:id="@+id/DLalrmsholder"
            android:layout_toRightOf="@id/IMR"
            android:layout_alignTop="@id/DLalrms"
            android:layout_alignBottom="@id/DLalrms">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/DLalrmsResult"
                android:text="-"

                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="-"

                android:id="@+id/DLalrmsBaseline"
                android:gravity="center"/>

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/DLalrmsRequired"
                android:text="TBC"

                android:gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/DLalrmsholder"
            android:background="#000000"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="Single Leg"
            android:layout_below="@id/DLalrmsholder"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:id="@+id/SL"
            android:gravity="center"
            android:layout_gravity="start"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="15sp"
            android:id="@+id/SLtestResult"
